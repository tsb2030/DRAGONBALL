<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="500.0"
	style="-fx-background-color: #ffffe4;"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="eye.game.follow.engPopupController">
	<children>
		<Button id="shadow" fx:id="closeBtn" layoutX="295.0"
			layoutY="400.0" mnemonicParsing="false" onAction="#closePopUp"
			prefHeight="44.0" prefWidth="125.0"
			style="-fx-background-color: #50af49; -fx-background-radius: 30;"
			text="그만하기" textFill="WHITE" AnchorPane.rightAnchor="100.0">
			<font>
				<Font name="a소나무M" size="21.0" />
			</font>
		</Button>
		<Label layoutX="157.0" layoutY="70.0" text="게임 성공!">
			<font>
				<Font name="A소나무M" size="44.0" />
			</font>
		</Label>
		<Button id="shadow" fx:id="restartPopUpBtn" layoutX="99.0"
			layoutY="400.0" mnemonicParsing="false" onAction="#numGameRestart"
			prefHeight="44.0" prefWidth="125.0"
			style="-fx-background-color: #50af49; -fx-background-radius: 30;"
			text="다시하기" textFill="WHITE" AnchorPane.leftAnchor="100.0">
			<font>
				<Font name="A소나무M" size="21.0" />
			</font>
		</Button>
		<Text fx:id="result" layoutX="176.0" layoutY="170.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="게임 시간 : x초">
			<font>
				<Font name="a소나무L" size="22.0" />
			</font>
		</Text>
		<Pane id="backBorder" layoutX="164.0" layoutY="211.0"
			prefHeight="150.0" prefWidth="180.0" />
		<Label layoutX="235.0" layoutY="231.0" text="기록">
			<font>
				<Font name="a소나무L" size="18" />
			</font>
		</Label>
		<Label fx:id="first" layoutX="215.0" layoutY="265.0" text="1위 1.11초">
			<font>
				<Font name="a소나무L" size="16"/>
			</font>
		</Label>
		<Label fx:id="second" layoutX="215.0" layoutY="290.0" text="2위 2.11초">
			<font>
				<Font name="a소나무L" size="16"/>
			</font>
		</Label>
		<Label fx:id="third" layoutX="215.0" layoutY="315.0" text="3위 3.11초">
			<font>
				<Font name="a소나무L" size="16"/>
			</font>
		</Label>
	</children>
</AnchorPane>
